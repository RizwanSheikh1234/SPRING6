<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
    xmlns="https://jakarta.ee/xml/ns/persistence/orm"
    version="3.1">


    <entity class="com.model.Product" access="PROPERTY">
        <table name="product"/>

        <attributes>
            <id name="id">
                <column name="id"/>
            </id>

            <basic name="title">
                <column name="title" nullable="false"/>
            </basic>

            <basic name="category">
                <column name="category"/>
            </basic>

            <basic name="pdate">
                <column name="pdate"/>
            </basic>
        </attributes>
    </entity>

</entity-mappings>
